{
  "name": "redwoodjs-stripe",
  "version": "0.0.1",
  "description": "A Redwood/Stripe integration made easy",
  "type": "module",
  "main": "./dist/cjs/api/index.js",
  "exports": {
    ".": {
      "import": "./dist/api-mjs/index.js",
      "require": "./dist/api-cjs/index.js"
    },
    "./api": {
      "import": "./dist/api-mjs/index.js",
      "require": "./dist/api-cjs/index.js"
    },
    "./web": {
      "import": "./dist/web-mjs/index.js",
      "require": "./dist/web-cjs/index.js"
    }
  },
  "targets": {
    "api-cjs": {
      "source": "src/api/index.js",
      "isLibrary": true,
      "context": "node",
      "outputFormat": "commonjs"
    },
    "web-cjs": {
      "source": "src/web/index.js",
      "isLibrary": true,
      "context": "browser",
      "outputFormat": "commonjs"
    },
    "api-mjs": {
      "source": "src/api/index.js",
      "isLibrary": true,
      "context": "node",
      "outputFormat": "esmodule"
    },
    "web-mjs": {
      "source": "src/web/index.js",
      "isLibrary": true,
      "context": "browser",
      "outputFormat": "esmodule"
    }
  },
  "scripts": {
    "watch": "parcel watch",
    "build": "parcel build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chrisvdm/redwoodjs-stripe.git"
  },
  "keywords": [
    "redwood",
    "stripe"
  ],
  "author": "Chrisvdm",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chrisvdm/redwoodjs-stripe/issues"
  },
  "homepage": "https://github.com/chrisvdm/redwoodjs-stripe#readme",
  "devDependencies": {
    "@redwoodjs/web": "1.x",
    "parcel": "latest",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "dependencies": {
    "@stripe/stripe-js": "1.29.0",
    "stripe": "^8.220.0"
  },
  "peerDependencies": {
    "@redwoodjs/core": "1.x",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  }
}
